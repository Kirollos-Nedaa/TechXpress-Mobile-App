{
  "expo": {
    "name": "TechXpress",
    "slug": "TechXpress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "techxpress",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.keronedaa.TechXpress"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#11113D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.keronedaa.TechXpress"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 1024,
          "resizeMode": "contain",
          "backgroundColor": "#11113D"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98402716-73f2-4caf-b755-7ee47d7a8d39"
      }
    }
  }
}
